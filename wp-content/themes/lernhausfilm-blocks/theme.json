{
    "$schema": "https://schemas.wp.org/wp/6.1/theme.json",
    "version": 2,
    "settings": {
        "appearanceTools": true,
        "layout": {
            "contentSize": "800px",
            "wideSize": "1000px"
        },
        "spacing": {
            "units": [
                "%",
                "px",
                "em",
                "rem",
                "vh",
                "vw"
            ]
        },
        "typography": {
            "fontFamilies": [
                {
                    "fontFamily": "system-ui, sans-serif",
                    "name": "System Font",
                    "slug": "system"
                }
           ]
        }
    },
    "templateParts": [
        {
            "area": "header",
            "name": "header"
        },
        {
            "area": "footer",
            "name": "footer"
        }
    ],
    "styles": {
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system)"		
        }
	}
}